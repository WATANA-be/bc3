<%= form_for @bookmarks , url: {action: :create} do |f| %>
    <%= f.label :title, 'タイトル' %>
    <%= f.text_field :title %>
    <%= f.label :author, '作者' %>
    <%= f.text_field :author %>
    <%= f.label :url, 'Discord URL' %>
    <%= f.text_field :url %>
    <%= f.label :phrase, 'フレーズ' %>
    <%= f.text_field :phrase %>
    <%= f.submit '登録' %>
 <% end %>